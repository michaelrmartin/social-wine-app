@wines.map do |wine|
  {
    id: wine.id,
    name: wine.name,
    blend: wine.blend,
    price: wine.price,
    style: wine.style,
    description: wine.description,
    users: wine.users, 
    origin: wine.origin,
    country: wine.origin.country,
    category: wine.categories,
    friendly_created_at: wine.friendly_created_at,
  }
end
